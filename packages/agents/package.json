{
  "name": "@argus/agents",
  "version": "1.0.0",
  "description": "Multi-agent AI system for Solana token analysis and protection",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/start.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "start": "node dist/start.js",
    "start:dev": "tsx scripts/start.ts",
    "lint": "eslint src/",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "solana",
    "ai",
    "agents",
    "token-analysis",
    "scam-detection",
    "bitnet",
    "multi-agent"
  ],
  "author": "Argus AI",
  "license": "MIT",
  "dependencies": {
    "@solana/web3.js": "^1.87.6",
    "bs58": "^6.0.0",
    "dotenv": "^17.2.3",
    "eventemitter3": "^5.0.1",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "eslint": "^8.54.0",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.2",
    "vitest": "^1.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
